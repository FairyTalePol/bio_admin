namespace Admin\ClientBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;


/**
 * @MongoDB\Document
 */
class Message
{
  /**
   * @MongoDB\Id
   */
  protected $id;

  /**
   * @MongoDB\Field
   */
  protected $data;

  /**
   * @MongoDB\Int
   */
  protected $db_role_id;

  /**
   * Get id
   *
   * @return id $id
   */
  public function getId()
  {
    return $this->id;
  }

  /**
   * Set data
   *
   * @param string $data
   * @return self
   */
  public function setData($data)
  {
    $this->data = $data;
    return $this;
  }

  /**
   * Get data
   *
   * @return string $data
   */
  public function getData()
  {
    return $this->data;
  }

    /**
     * Set dbRoleId
     *
     * @param int $dbRoleId
     * @return self
     */
    public function setDbRoleId($dbRoleId)
    {
        $this->db_role_id = $dbRoleId;
        return $this;
    }

    /**
     * Get dbRoleId
     *
     * @return int $dbRoleId
     */
    public function getDbRoleId()
    {
        return $this->db_role_id;
    }
}
