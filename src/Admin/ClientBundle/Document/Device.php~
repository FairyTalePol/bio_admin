namespace Admin\ClientBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;


/**
 * @MongoDB\Document
 */
class Device
{
  /**
   * @MongoDB\Id
   */
  protected $id;

  /**
   * @MongoDB\Hash
   */
  protected $data;

  /**
   * @MongoDB\Int
   * @MongoDB\Index(unique=true, order="asc")
   */
  protected $item_id;

}
