      }

      @font-face {
        font-family: OpenSansBold;
        src: url("{{ asset('bundles/adminclient/fonts/index/OpenSans-Bold-webfont.eot') }}");
        src: url("{{ asset('bundles/adminclient/fonts/index/OpenSans-Bold-webfont.eot?#iefix') }}") format("embedded-opentype"), url("{{ asset('bundles/adminclient/fonts/index/OpenSans-Bold-webfont.woff') }}") format("woff"), url("{{ asset('bundles/adminclient/fonts/index/OpenSans-Bold-webfont.ttf') }}") format("truetype") !important
