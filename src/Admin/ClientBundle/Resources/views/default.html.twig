      }

      @font-face {
        font-family: OpenSansRegular;
        src: url("{{ asset('bundles/adminclient/fonts/index/OpenSans-Regular-webfont.eot') }}");
        src: url("{{ asset('bundles/adminclient/fonts/index/OpenSans-Regular-webfont.eot?#iefix') }}") format("embedded-opentype"), url("{{ asset('bundles/adminclient/fonts/index/OpenSans-Regular-webfont.woff') }}") format("woff"), url("{{ asset('bundles/adminclient/fonts/index/OpenSans-Regular-webfont.ttf') }}") format("truetype") !important
      }

      @font-face {
        font-family: OpenSansSemibold;
