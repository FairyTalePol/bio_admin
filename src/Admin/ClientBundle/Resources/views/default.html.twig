  <script>
    var loginError = $('div').hasClass('_getLoginError');
    var contact_tel = $('#contact_tel');
    $('.carousel').carousel({interval: 3000});
    $('.modal-notice').modal('show');
    //        $("#contact_tel").mask("+99999999999?99");
    contact_tel.on('focus', function () {
      if (contact_tel.val() === "") {
        contact_tel.val('+');
      }
    });
    contact_tel.on('blur', function () {
      if (contact_tel.val() === "+") {
        contact_tel.val('');
      }
    });

    if (window.location.hash === '#login' || loginError) {
      $('.modal-login-window').modal('show');
    }
  </script>
